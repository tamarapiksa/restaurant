  <div class="orderdiv">
    <body class="orderbod">
     <div class="menubod">
    <h2 class="tablehead">Add Order</h2><br><br><br><br><br>
<form action="/orders" method="POST">
  <select name="order[food_id]">
    <% Food.all.each do |food| %>
      <option value="<%= food.id %>"><%= food.name %></option>
    <% end %>
  </select>
  <select name="order[party_id]">
    <% Party.all.each do |party| %>
      <option value="<%= party.id %>"><%= party.table_number %></option>
    <% end %>
  </select>
  
   <input type="submit">
 </form>
</div>
</body>
</div> 



